{
  "name": "@visionworksco/react-middleware",
  "version": "1.6.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "React middleware",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:visionworksco/react-middleware.git"
  },
  "author": "VisionWorks",
  "homepage": "https://github.com/visionworksco/react-middleware",
  "bugs": {
    "url": "https://github.com/visionworksco/react-middleware/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run compile && barrelsby --config barrelsbyrc.json && tsc -p tsconfig.build.json && prettier --write ./src/index.ts",
    "clean": "rimraf dist && rimraf coverage && rimraf ./src/index.ts",
    "commit": "git add . && git commit -m 'feat: :rocket:'",
    "compile": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "stylelint '**/*.{css,scss,less}' --fix && eslint '**/*.{js,jsx,ts,tsx}' --fix",
    "lint:license": "license-checker --production --json --out licenses.json",
    "lint:node_modules": "./nm-lint.sh",
    "lint:staged": "lint-staged",
    "npm:publish": "git push --follow-tags origin master && npm publish",
    "npm:release:first": "standard-version --first-release",
    "npm:release:major": "standard-version --release-as major",
    "npm:release:minor": "standard-version --release-as minor",
    "npm:release:patch": "standard-version --release-as patch",
    "setup": "husky install",
    "test": "npm run test:unit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "jest --config jest.config.js --group=unit --coverage",
    "test:dev": "jest --config jest.config.js --group=unit --watch",
    "test:integration": "jest --config jest.config.integration.js --group=integration",
    "test:staged": "jest --config jest.config.integration.js --onlyChanged",
    "test:unit": "jest --config jest.config.js --group=unit"
  },
  "keywords": [
    "visionworks, react, typescript"
  ],
  "dependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.2",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt-cli": "^16.0.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.15.2",
    "barrelsby": "^2.3.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.1",
    "jest-extended": "^1.2.0",
    "jest-mock-extended": "^2.0.4",
    "jest-runner-groups": "^2.1.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-package": "^1.3.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^2.5.4",
    "standard-version": "^9.3.2",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "ts-jest": "^27.1.1",
    "typescript": "4.0.3"
  },
  "types": "dist/index.d.ts"
}
